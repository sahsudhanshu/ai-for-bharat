# Environment variables for OceanAI Mobile App
# Copy this to .env and fill in your values

# Backend API (AWS API Gateway URL)
# Leave empty to use demo mode with mock data
EXPO_PUBLIC_API_URL=

# Cognito (same pool/client as website)
EXPO_PUBLIC_COGNITO_USER_POOL_ID=ap-south-1_XXXXXXXXX
EXPO_PUBLIC_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# (Optional) For EAS Builds
# EXPO_PUBLIC_ENV=development
